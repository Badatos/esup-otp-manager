#administration.content-unit(hidden='true')
    #totp_method.method(hidden='true')
        h1.page-header #{messages.api.totp}
            span#totp_activate_label.label.label-success(style="display: none;") Activée
            span#totp_deactivate_label.label.label-danger(style="display: none;") Désactivée
        .method-content
            button#totp_activate(onclick='activate_method_admin(this);',class='btn btn-success') Activer #{messages.api.totp}
            button#totp_deactivate(onclick='deactivate_method_admin(this);',class='btn btn-danger') Désactiver #{messages.api.totp}
        p
            | Activer le transport SMS : oui
            span#totp_activate_sms_transport.glyphicon.glyphicon-unchecked(aria-hidden='true', onclick='activate_method_transport(this);')
            |  non
            span#totp_deactivate_sms_transport.glyphicon.glyphicon-unchecked(aria-hidden='true', onclick='deactivate_method_transport(this);')
        p
            | Activer le transport Mail : oui
            span#totp_activate_mail_transport.glyphicon.glyphicon-unchecked(aria-hidden='true', onclick='activate_method_transport(this);')
            |  non
            span#totp_deactivate_mail_transport.glyphicon.glyphicon-unchecked(aria-hidden='true', onclick='deactivate_method_transport(this);')
