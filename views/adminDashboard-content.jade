#users.content-unit(hidden='true')
    | Nom d'utilisateur:
    input#userInput(type='text')
    button(onclick='get_user();',class='btn btn-success') Valider
    #userInfo(hidden='true')
        #totp_admin
            .method-content
                h1.page-header #{messages.api.totp}
                    span#admin_totp_activate_label.label.label-success(style="display: none;") Activée
                    span#admin_totp_deactivate_label.label.label-danger(style="display: none;") Désactivée
                p
                | Méthode permettant de générer des codes uniques de 6 chiffres
                button#admin_totp_activate(onclick='activate_method_user_admin(this);',class='btn btn-success') Activer #{messages.api.totp}
                button#admin_totp_deactivate(onclick='deactivate_method_user_admin(this);',class='btn btn-danger') Désactiver #{messages.api.totp}
                #totp_qrCode
                #totp_secret
                button(onclick='admin_generate_totp();') Générer nouveau QrCode
                button(onclick='admin_delete_totp_secret();') Supprimer le QrCode
        #random_code_admin
            .method-content
                h1.page-header #{messages.api.random_code}
                    span#admin_random_code_activate_label.label.label-success(style="display: none;") Activée
                    span#admin_random_code_deactivate_label.label.label-danger(style="display: none;") Désactivée
